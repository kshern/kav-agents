{
  "name": "react-ts-monorepo",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "pnpm -C packages/chzj build",
    "build:all": "pnpm -C packages/core build && pnpm -C packages/web build",
    "build:core": "pnpm -C packages/core build",
    "build:web": "pnpm -C packages/web build",
    "build:datasources": "pnpm -C packages/datasources build",
    "dev:web": "pnpm -C packages/web dev",
    "dev:core": "pnpm -C packages/core dev",
    "cli:wencai-news": "pnpm -C packages/datasources cli:wencai-news",
    "cli:wencai-news:batch": "pnpm -C packages/datasources cli:wencai-news:batch",
    "cli:tushare-call": "pnpm -C packages/datasources cli:tushare-call",
    "lint:all": "pnpm -C packages/web lint",
    "lint:web": "pnpm -C packages/web lint",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "gen:docs": "node scripts/gen-docs.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.0.7",
    "@types/react": "^19.1.8",
    "@vitejs/plugin-react": "^4.5.2",
    "prettier": "^3.6.2",
    "vite": "^7.0.6"
  }
}
